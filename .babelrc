{
  "presets": [
    [
      "@babel/preset-react",
      {
        "runtime": "automatic"
      }
    ],
    [
      "@babel/preset-env",
      {
        // 描述您为项目支持/目标的环境
        "targets": {
          "browsers": ["> 1%", "last 2 versions"]
        },
        "corejs": 2, //新版本需要指定核⼼库版本
        // "usage"| "entry"| false，默认为false
        "useBuiltIns": "usage" //按需注⼊
      }
    ]
  ]
}
